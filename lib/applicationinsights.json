{
  "connectionString": "${file:/mnt/secrets/idam-idam/app-insights-connection-string}",
  "role": {
    "name": "idam-testing-support-api"
  },
  "preview": {
    "roleNameOverrides": [
      {
        "httpPathPrefix": "/health",
        "roleName": "idam-testing-support-api (health)"
      }
    ],
    "sampling": {
      "overrides": [
        {
          "telemetryType": "request",
          "attributes": [
            {
              "key": "http.url",
              "value": "https?://[^/]+/health",
              "matchType": "regexp"
            }
          ],
          "percentage": 1
        }
      ]
    }
  },
  "instrumentation": {
    "micrometer": {
      "enabled": false
    }
  }
}
